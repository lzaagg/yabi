<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/bin/ccg-prepautopcap" toolName="prepautopcap" displayName="assembly listing" description="Takes the output from crossmatch and assembles files for autopcap">

    <inputFile/>
    <outputFile/>


    <inputFiletypes requiresInput="true">
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false" symlinkOutputDir="true">
        <extension>list</extension>
    </outputFiletypes>


    <parameterList>
        <parameter rank="1" switchUse="valueOnly" switch="inputFiles" mandatory="yes" filter="prependUserDir" inputFile="no" outputFile="no" value=""/>
        <parameter rank="2" switchUse="valueOnly" switch="outFile" mandatory="yes" filter="" inputFile="no" outputFile="yes" value="output.list"/>
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
