<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/bin/curl" toolName="fetchgenbankrecord" displayName="select genbank record" description="Download a genbank record for the supplied accession or GI number.">

    <inputFiletypes requiresInput="false" acceptsInput="false">
        <extension>*</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>gb</extension>
    </outputFiletypes>


    <inputFile/>
    <outputFile/>

    <parameterList>
        <parameter rank="1" switchUse="both" switch="-o" mandatory="no" inputFile="no" outputFile="no"  value="output.gb"/>
        <parameter rank="" switchUse="switchOnly" switch="-k" mandatory="no" inputFile="no" outputFile="no" value=""/>
        <parameter rank="" switchUse="valueOnly" switch="url" mandatory="yes" inputFile="no" outputFile="no" value=""/>
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
