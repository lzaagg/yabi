<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/bl2seqWrapper" toolName="bl2seq" displayName="bl2seq" description="Align two sequences against each other.">

    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension mustExist="false">bls</extension>
    </outputFiletypes>

    <parameterList>
      <parameter rank="1" switchUse="both" switch="-i" mandatory="yes" inputFile="yes"    outputFile="no"  value="">
          <acceptedExtensionList>
              <acceptedExtension>fa</acceptedExtension>
              <acceptedExtension>fna</acceptedExtension>
              <acceptedExtension>faa</acceptedExtension>
          </acceptedExtensionList>
      </parameter>
      <parameter rank="2" switchUse="both" switch="-j" mandatory="yes" inputFile="yes"    outputFile="no"  value="">
          <acceptedExtensionList>
              <acceptedExtension>fa</acceptedExtension>
              <acceptedExtension>fna</acceptedExtension>
              <acceptedExtension>faa</acceptedExtension>
          </acceptedExtensionList>
      </parameter>
      <parameter rank="" switchUse="both" switch="-p" mandatory="yes" inputFile="no"    outputFile="no"  value=""/>
      <parameter rank=""  switchUse="both" switch="-e" mandatory="no"  inputFile="no"    outputFile="no"  value=""/>
      <parameter rank=""  switchUse="both" switch="-l" mandatory="no"  inputFile="maybe" outputFile="no"  value=""/>
      <parameter rank=""  switchUse="both" switch="-F" mandatory="no"  inputFile="no"    outputFile="no"  value=""/>
      <parameter rank=""  switchUse="both" switch="-M" mandatory="no"  inputFile="no"    outputFile="no"  value=""/>
      <parameter rank=""  switchUse="both" switch="-W" mandatory="no"  inputFile="no"    outputFile="no"  value=""/>
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
