      <parameter rank=""  switchUse="combined"   switch="-loopgap="      mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-terminalgap="  mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-helixendin="   mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-helixendout="  mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-strandendin="  mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-strandendout=" mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-outputtree="   mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-seed="         mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-kimura"        mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-tossgaps"      mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-bootlabels="   mandatory="no"  inputFile="no"    value="" />
    </parameterList>

    <parameterAliasList>
      <parameterAlias switchAlias="-BOOTSTRAP" switch="-bootstrap=" switchValue="1000" />
    </parameterAliasList>



