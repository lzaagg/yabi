      <parameter rank=""  switchUse="combined"   switch="-hgapresidues=" mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-maxdiv="       mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-type="         mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-transweight="  mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-profile"       mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="combined"   switch="-newtree1="     mandatory="no"  inputFile="yes"   value=""/>
      <parameter rank=""  switchUse="combined"   switch="-newtree2="     mandatory="no"  inputFile="yes"   value=""/>
      <parameter rank=""  switchUse="combined"   switch="-usetree1="     mandatory="no"  inputFile="yes"   value=""/>
      <parameter rank=""  switchUse="combined"   switch="-usetree2="     mandatory="no"  inputFile="yes"   value=""/>
      <parameter rank=""  switchUse="switchOnly" switch="-sequences"     mandatory="no"  inputFile="no"    value="" />
