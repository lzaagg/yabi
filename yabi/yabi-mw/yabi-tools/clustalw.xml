      <parameter rank=""  switchUse="combined"   switch="-usetree="      mandatory="no"  inputFile="yes"   value=""/>
      <parameter rank=""  switchUse="switchOnly" switch="-nosecstr1"     mandatory="no"  inputFile="no"    value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-nosecstr2"     mandatory="no"  inputFile="no"    value="" />
