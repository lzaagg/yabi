<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/extractseqWrapper" toolName="extractseq" displayName="extractseq" description="Allows you to specify one or more regions of a sequence to extract sub-sequences from to build up a contiguous resulting sequence">

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
        <extension>seq</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>fa</extension>
    </outputFiletypes>

    <inputFile/>
    <outputFile/>

    <parameterList>
      <parameter rank=""  switchUse="both"       switch="-sequence" mandatory="yes" inputFile="yes" value="" />
      <parameter rank=""  switchUse="both"       switch="-outseq"   mandatory="no"  inputFile="no"  value="" sourceParam="-sequence" appendString=".fa" />
      <parameter rank=""  switchUse="both"       switch="-regions"  mandatory="yes" inputFile="no" value=""  />
      <parameter rank=""  switchUse="switchOnly" switch="-separate" mandatory="no"  inputFile="no" value=""  />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
