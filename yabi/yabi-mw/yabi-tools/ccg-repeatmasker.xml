<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job jobType="ccg" toolPath="/usr/local/grendel/bin/repeatmaskerWrapper" toolName="ccg-repeatmasker" displayName="ccg-repeatmasker" description="RepeatMasker is a program that screens DNA sequences for interspersed repeats and low complexity DNA sequences">

    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>cat</extension>
        <extension>log</extension>
        <extension>masked</extension>
        <extension>out</extension>
        <extension>tbl</extension>
    </outputFiletypes>
    

    <parameterList>
      <parameter rank=""  switchUse="both"       switch="-species"  mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-lib"      mandatory="no"  inputFile="yes" value="" />
      <parameter rank=""  switchUse="both"       switch="-div"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-gc"       mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-frag"     mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-maxsize"  mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-nolow"    mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-int"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-norna"    mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-alu"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-is_only"  mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-is_clip"  mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-no_is"    mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-rodspec"  mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-primspec" mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-nocut"    mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-a"        mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-inv"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-cut"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-small"    mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-xsmall"   mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-x"        mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-poly"     mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-ace"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-gff"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-u"        mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-xm"       mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-fixed"    mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-no_id"    mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-excln"    mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-engine"   mandatory="no"  inputFile="no"  value="" />
      <parameter rank="" switchUse="valueOnly"  switch="notUsed"   mandatory="yes" inputFile="yes" value="">
        <outputExtension>cat</outputExtension>
        <outputExtension>log</outputExtension>
        <outputExtension>masked</outputExtension>
        <outputExtension>out</outputExtension>
        <outputExtension>tbl</outputExtension>
      </parameter>
    </parameterList>

    <parameterAliasList>
      <parameterAlias switchAlias="-s"           switch="-speed"   switchValue="slow"       />
      <parameterAlias switchAlias="-q"           switch="-speed"   switchValue="quick"      />
      <parameterAlias switchAlias="-qq"          switch="-speed"   switchValue="rush"       />
      <parameterAlias switchAlias="-art"         switch="-species" switchValue="cow"        />
      <parameterAlias switchAlias="-artiodactyl" switch="-species" switchValue="cow"        />
      <parameterAlias switchAlias="-ar"          switch="-species" switchValue="arabidopsis"/>
