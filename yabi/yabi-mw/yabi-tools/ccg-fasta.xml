<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job jobType="ccg" toolPath="/usr/local/grendel/bin/fastaWrapper" toolName="ccg-fasta" displayName="ccg-fasta" description="fasta on the ccg cluster">

    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
        <extension>seq</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>fsa</extension>
    </outputFiletypes>
    
    <parameterList>
      <parameter rank="0" switchUse="valueOnly" switch="program"   mandatory="yes" inputFile="no" value=""/>
      <parameter rank="1" switchUse="valueOnly" switch="inputFile" mandatory="yes" inputFile="yes" value=""/>
      <parameter rank="2" switchUse="valueOnly" switch="dataSet"   mandatory="yes" inputFile="maybe" value=""/>
      <parameter rank="" switchUse="both" switch="-b" mandatory="no" inputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-d" mandatory="no" inputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-E" mandatory="no" inputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-F" mandatory="no" inputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-s" mandatory="no" inputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-r" mandatory="no" inputFile="no" value=""/>
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
