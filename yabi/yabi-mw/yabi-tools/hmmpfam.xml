<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/hmmpfamWrapper" toolName="hmmpfam" displayName="hmmpfam" description="hmmpfam">

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
        <extension>seq</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
    </outputFiletypes>

    <inputFile/>
    <outputFile/>

    <parameterList>
      <parameter rank="1" switchUse="valueOnly"  switch="dataSet"     mandatory="yes" inputFile="maybe" outputFile="no"  value="">
        <globus_prefix>PFAM_DIR</globus_prefix>
      </parameter>
      <parameter rank="2" switchUse="valueOnly"  switch="inputFile"     mandatory="yes" inputFile="maybe" outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-n"          mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-A"          mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-E"          mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-T"          mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-Z"          mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="--acc"       mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="--compat"    mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="--cut_ga"    mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="--cut_nc"    mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="--cut_tc"    mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="--domE"      mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="--domT"      mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="--forward"   mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="--informat"  mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="--null2"     mandatory="no"  inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="--xnu"       mandatory="no"  inputFile="no"    outputFile="no"  value="" />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
