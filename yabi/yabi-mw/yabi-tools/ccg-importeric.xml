<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/bin/ccg-importeric" toolName="ccg-importeric" displayName="import into SOE" description="Imports files into Son of Eric">

    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>bls</extension>
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
        <extension>gff</extension>
        <extension>masked</extension>
        <extension>seq</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
    </outputFiletypes>

    <parameterList>
      <parameter rank="1" switchUse="both" switch="-i" mandatory="yes" inputFile="yes" outputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-t" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-s" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-l" mandatory="no" inputFile="no" outputFile="no" value=""/>
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
