<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/prointblast2htmlWrapper" toolName="prointblast2html" displayName="pi-blast prettyprint" description="HTML formats the output from pi-blast">

    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>bls</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>html</extension>
    </outputFiletypes>


    <parameterList>
      <parameter rank=""  switchUse="valueOnly"  switch="inputFiles" mandatory="no" inputFile="yes" inputType="listoffiles" outputFile="no"  value="" />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
