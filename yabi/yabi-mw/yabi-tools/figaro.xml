<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/figaroWrapper" toolName="figaro" displayName="figaro" description="remove vector from random shotgun-based reads without prior knowledge of the vector sequence itself">

    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>seq</extension>
        <extension>qual</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension mustExist="true">summary</extension>
        <extension mustExist="true">vectorcuts</extension>
    </outputFiletypes>

    <parameterList>
      <parameter rank=""  switchUse="both" switch="-F" mandatory="yes" inputFile="yes" outputFile="no"  value="" />
      <parameter rank=""  switchUse="both" switch="-P" mandatory="yes" inputFile="no"  outputFile="no"  value="" />
      <parameter rank=""  switchUse="both" switch="-T" mandatory="no"  inputFile="no"  outputFile="no"  value="" />
      <parameter rank=""  switchUse="both" switch="-M" mandatory="no"  inputFile="no"  outputFile="no"  value="" />
      <parameter rank=""  switchUse="both" switch="-E" mandatory="no"  inputFile="no"  outputFile="no"  value="" />
      <parameter rank=""  switchUse="both" switch="-V" mandatory="no"  inputFile="no"  outputFile="no"  value="" />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
