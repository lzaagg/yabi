<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/prettyplotWrapper" toolName="prettyplot" displayName="prettyplot" description="Displays aligned sequences, with colouring and boxing">

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
        <extension>seq</extension>
        <extension>aln</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>png</extension>
        <extension>ps</extension>
    </outputFiletypes>
    <inputFile/>
    <outputFile/>

    <parameterList>
      <parameter rank=""  switchUse="both"       switch="-sequences"       mandatory="yes" inputFile="yes" value="" />
      <parameter rank=""  switchUse="both"       switch="-graph"           mandatory="yes" inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-residuesperline" mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-resbreak"        mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-ccolours"        mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-noccolours"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-cidentity"       mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-csimilarity"     mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-cother"          mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-docolour"        mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-title"           mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-notitle"         mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-shade"           mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-pair"            mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-identity"        mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-box"             mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-nobox"           mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-boxcol"          mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-boxcolval"       mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-name"            mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-maxnamelen"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-number"          mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-nonumber"        mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-nolistoptions"   mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-plurarity"       mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-consensus"       mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-collision"       mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-nocollision"     mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-alternative"     mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-matrixfile"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-showscore"       mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-portrait"        mandatory="no"  inputFile="no"  value="" />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
