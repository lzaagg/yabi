<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/autopcapWrapper" toolName="ivec-autopcap" displayName="ivec-autopcap" description="Sequence Assembly">
    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>list</extension>
        <extension>gz</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false" symlinkOutputDir="true">
        <extension>*</extension>
    </outputFiletypes>



    <parameterList>
      <parameter rank=""  switchUse="valueOnly"  switch="inputFiles"          mandatory="yes" inputFile="yes" inputType="listoffiles" outputFile="no"  value="" primaryExtension="list" filter="removePath"/>
      <parameter rank=""  switchUse="both"       switch="-d"               mandatory="no"  inputFile="no"    inputType=""            outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-l"               mandatory="no"  inputFile="no"    inputType=""            outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-m"               mandatory="no"  inputFile="no"    inputType=""            outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-p"               mandatory="no"  inputFile="no"    inputType=""            outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-s"               mandatory="no"  inputFile="no"    inputType=""            outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-t"               mandatory="no"  inputFile="no"    inputType=""            outputFile="no"  value="" />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
