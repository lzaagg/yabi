<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/getorfWrapper" toolName="getorf" displayName="getorf" description="Finds and extracts open reading frames (ORFs)">

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
        <extension>seq</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>fa</extension>
    </outputFiletypes>

    <inputFile/>
    <outputFile/>

    <parameterList>
      <parameter rank=""  switchUse="both"       switch="-sequence"     mandatory="yes" inputFile="yes" value="" />
      <parameter rank=""  switchUse="both"       switch="-outseq"       mandatory="no"  inputFile="no"  value="" sourceParam="-sequence" appendString=".fa" /> 
      <parameter rank=""  switchUse="both"       switch="-table"        mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-minsize"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-maxsize"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-find"         mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-methionine"   mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-nomethionine" mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-circular"     mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-reverse"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-noreverse"    mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-flanking"     mandatory="no"  inputFile="no"  value="" />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
