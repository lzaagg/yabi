<?xml version="1.0"?>
<job>
  <factoryEndpoint xmlns:gram="http://www.globus.org/namespaces/2004/10/gram/job" xmlns:wsa="http://schemas.xmlsoap.org/ws/2004/03/addressing">
    <wsa:Address>https://xe-ng2.ivec.org:8443/wsrf/services/ManagedJobFactoryService</wsa:Address>
    <wsa:ReferenceProperties>
      <gram:ResourceID>PBS</gram:ResourceID>
    </wsa:ReferenceProperties>
  </factoryEndpoint>
  <executable>blastall</executable>
  <directory>/${GLOBUS_SCRATCH_DIR}/#JOB_DIR#</directory>
  <argumentPlaceholder/>
  <argument>-o</argument>
  <argument>blastall_output.bls</argument>
  <stdout>stdoutlog</stdout>
  <stderr>stderr.log</stderr>
  <count>8</count>
  <queue>normal</queue>
  <maxWallTime>240</maxWallTime>
  <maxMemory>16384</maxMemory>
  <jobType>single</jobType>
  <extensions>
    <module>blast</module>
  </extensions>
</job>
