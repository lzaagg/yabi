<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/bin/sort" toolName="sort" displayName="sort" description="Arrange elements of a sequence in order according to some criterion">

    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>txt</extension>
        <extension>fa</extension>
        <extension>seq</extension>
        <extension>fna</extension>
        <extension>faa</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>fa</extension>
    </outputFiletypes>

    <parameterList>
      <parameter rank="1" switchUse="valueOnly" switch="notUsed" mandatory="yes" inputFile="yes" outputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-o" mandatory="no" inputFile="no" outputFile="no" value="sort.fa"/>
      <parameter rank="" switchUse="switchOnly" switch="-b" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-d" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-f" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-g" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-i" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-M" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-n" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-r" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-c" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-m" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-s" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-S" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="both" switch="-t" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-u" mandatory="no" inputFile="no" outputFile="no" value=""/>
      <parameter rank="" switchUse="switchOnly" switch="-z" mandatory="no" inputFile="no" outputFile="no" value=""/>
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
