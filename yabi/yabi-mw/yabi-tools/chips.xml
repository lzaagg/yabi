<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/chipsWrapper" toolName="chips" displayName="chips" description="produces a file containing codon usage statistics">

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
        <extension>seq</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>out</extension>
    </outputFiletypes>

    <inputFile/>
    <outputFile/>

    <parameterList>
      <parameter rank=""  switchUse="both" switch="-seqall"    mandatory="yes" inputFile="yes" value="" />
      <parameter rank=""  switchUse="both" switch="-cfile"     mandatory="no"  inputFile="no"  value="" />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
