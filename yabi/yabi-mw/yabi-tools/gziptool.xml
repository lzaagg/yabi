<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/bin/gzip" toolName="gziptool" displayName="gzip tool" description="gzips the output of the previous node">

    <inputFile/>
    <outputFile/>


    <inputFiletypes requiresInput="true">
        <extension>*</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>txt</extension>
    </outputFiletypes>


    <parameterList>
        <parameter rank="1" switchUse="switchOnly" switch="-r" mandatory="no" inputFile="no" outputFile="no"  value=""/>
        <parameter rank="2" switchUse="switchOnly" switch="-9" mandatory="no" inputFile="no" outputFile="no" value=""/>
        <parameter rank="3" switchUse="switchOnly" switch="-q" mandatory="no" inputFile="no" outputFile="no" value=""/>
        <parameter rank="4" switchUse="valueOnly" switch="inputFiles" mandatory="no" filter="prependUserDir" inputFile="no" outputFile="no" value=""/>
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
