<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/runCAWrapper" toolName="runCA" displayName="celera" description="Run a whole-genome shotgun assembly using Celera Assembler">

    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>frg</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>*</extension>
    </outputFiletypes>


    <parameterList>
      <parameter rank=""  switchUse="both"       switch="-d"       mandatory="yes" inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-p"       mandatory="yes" inputFile="no"    outputFile="no"  value="" />
      <parameter rank=""  switchUse="both"       switch="-s"       mandatory="yes" inputFile="maybe" outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-help"    mandatory="no"  inputFile="no"  outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-version" mandatory="no"  inputFile="no"  outputFile="no"  value="" />
      <parameter rank=""  switchUse="switchOnly" switch="-fields"  mandatory="no"  inputFile="no"  outputFile="no"  value="" />
      <parameter rank=""  switchUse="valueOnly"  switch="fragFile" mandatory="yes" inputFile="yes" outputFile="no"  value="" />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
