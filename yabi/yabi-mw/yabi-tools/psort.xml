<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
	<job toolPath="/usr/local/grendel/bin/psortWrapper" toolName="psort" displayName="psort" description="Used for the prediction of protein localisation sites in cells, it reports the possibility for the input protein to be localised at each candidate site with additional information.">
		<inputFile/>
		<outputFile/>
		
		<inputFiletypes requiresInput="true" acceptsInput="true">
			<extension>fa</extension>
		</inputFiletypes>
		
		<outputFiletypes filePassThru="false">
			<extension>txt</extension>
		</outputFiletypes>
		
		<parameterList>
			<parameter rank="1" switchUse="valueOnly"  switch="notUsed"            mandatory="yes" inputFile="yes"   outputFile="no"  value=""/>
			<parameter rank=""  switchUse="switchOnly" switch="-p"                 mandatory="no"  inputFile="no"    outputFile="no"  value="" />
			<parameter rank=""  switchUse="switchOnly" switch="-n"                 mandatory="no"  inputFile="no"    outputFile="no"  value="" />
			<parameter rank=""  switchUse="both"       switch="-c"                 mandatory="no"  inputFile="no"    outputFile="no"  value="" />
			<parameter rank=""  switchUse="both"       switch="-d"                 mandatory="no"  inputFile="no"    outputFile="no"  value="" />
			<parameter rank=""  switchUse="both"       switch="-f"                 mandatory="no"  inputFile="no"    outputFile="no"  value="" />
			<parameter rank=""  switchUse="switchOnly" switch="-e"                 mandatory="no"  inputFile="no"    outputFile="no"  value="" />
			<parameter rank=""  switchUse="both"       switch="-o"                 mandatory="no"  inputFile="no"    outputFile="no"  value="" />
			<parameter rank=""  switchUse="switchOnly" switch="-v"                 mandatory="no"  inputFile="no"    outputFile="no"  value="" />
		</parameterList>
		
		<parameterAliasList>
			<parameterAlias switchAlias="--positive"       switch="-p"   switchValue=""                    />
			<parameterAlias switchAlias="--negative"       switch="-n"   switchValue=""                    />
			<parameterAlias switchAlias="--cutoff"         switch="-c"   switchValue="useSwitchAliasValue" />
			<parameterAlias switchAlias="--divergent"      switch="-d"   switchValue="useSwitchAliasValue" />
			<parameterAlias switchAlias="--format"         switch="-f"   switchValue="useSwitchAliasValue" />
			<parameterAlias switchAlias="--exact"          switch="-e"   switchValue=""                    />
			<parameterAlias switchAlias="--output"         switch="-o"   switchValue="useSwitchAliasValue" />
			<parameterAlias switchAlias="--verbose"        switch="-v"   switchValue=""                    />
			
		</parameterAliasList>
		
		<grendel/>
		<priority/>
		<executionHost/>
		<batchNumber/>
		<status/>
		<startTime/>
		<stopTime/>
		
		<submittedTime/>
		<submitUser/>
		<submitLabel/>
	</job>
</baat>
