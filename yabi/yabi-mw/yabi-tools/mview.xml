<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/grendel/bin/mviewWrapper" toolName="mview" displayName="mview" description="mview">

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>aln</extension>
        <extension>bls</extension>
        <extension>clustal</extension>
        <extension>fa</extension>
        <extension>fna</extension>
        <extension>faa</extension>
        <extension>hssp</extension>
        <extension>mips</extension>
        <extension>msf</extension>
        <extension>multas</extension>
        <extension>pir</extension>
        <extension>txt</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
    </outputFiletypes>
    <inputFile/>
    <outputFile/>

    <parameterList>
      <parameter rank="1" switchUse="valueOnly" switch="notUsed"     mandatory="yes" inputFile="yes" value="" />
      <parameter rank=""  switchUse="both"      switch="-in"         mandatory="yes" inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"      switch="-ruler"      mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"      switch="-alignment"  mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"      switch="-consensus"  mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"      switch="-coloring"   mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"      switch="-threshold"  mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"      switch="-ignore"     mandatory="no"  inputFile="no"  value="" />
      <parameter rank=""  switchUse="both"      switch="-html"       mandatory="no"  inputFile="no"  value="" />
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
