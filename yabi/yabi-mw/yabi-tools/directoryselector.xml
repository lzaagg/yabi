    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="false" acceptsInput="false">
        <extension>*</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>*</extension>
    </outputFiletypes>

    <parameterList/>

