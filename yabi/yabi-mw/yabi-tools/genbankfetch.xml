<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="/usr/local/bin/ccg-genbankfetch" toolName="genbankfetch" displayName="get sequence" description="Accepts yabixml and fetches sequence from genbank.">

    <inputFiletypes requiresInput="true" acceptsInput="true">
        <extension>xml</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>xml</extension>
    </outputFiletypes>


    <inputFile/>
    <outputFile/>

    <parameterList>
        <parameter rank="1" switchUse="valueOnly" switch="inputFiles" mandatory="yes" inputFile="yes" outputFile="no"  value="">
            <acceptedExtensionList>
                <acceptedExtension>xml</acceptedExtension>
            </acceptedExtensionList>
        </parameter>
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>
