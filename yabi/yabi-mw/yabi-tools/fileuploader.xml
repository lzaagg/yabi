<?xml version="1.0"?>
<!DOCTYPE baat SYSTEM "http://ccg.murdoch.edu.au/dtd/baat.dtd">
<baat>
  <job toolPath="echo" toolName="fileuploader" displayName="upload file" description="Upload a file from your computer to your workspace directory.">

    <inputFile/>
    <outputFile/>

    <inputFiletypes requiresInput="false" acceptsInput="false">
        <extension>*</extension>
    </inputFiletypes>

    <outputFiletypes filePassThru="false">
        <extension>*</extension>
    </outputFiletypes>

    <parameterList>
        <parameter rank="1" switchUse="valueOnly" switch="files" mandatory="yes" inputFile="no" outputFile="no"  value="file uploaded"/>
    </parameterList>

    <parameterAliasList/>

    <grendel/>
    <priority/>
    <executionHost/>
    <batchNumber/>
    <status/>
    <startTime/>
    <stopTime/>
    <submittedTime/>
    <submitUser/>
    <submitLabel/>
  </job>
</baat>

